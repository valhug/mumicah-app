{
  "name": "mumicah-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:init": "tsx scripts/init-mongodb.ts",
    "db:seed": "tsx scripts/seed-database.ts",
    "db:cleanup": "tsx scripts/cleanup-database.ts",
    "db:setup": "pnpm run db:init && pnpm run db:seed",
    "test:setup": "tsx scripts/test-setup.ts",
    "mongodb:start": ".\\scripts\\start-mongodb.bat",
    "mongodb:stop": "docker stop mumicah-mongodb",
    "mongodb:remove": "docker rm mumicah-mongodb && docker volume rm mumicah-mongodb-data",
    "types:generate": "supabase gen types typescript --project-id $PROJECT_REF --schema public > types/supabase-generated.types.ts",
    "supabase:login": "supabase login",
    "supabase:init": "supabase init",
    "supabase:link": "supabase link --project-ref $PROJECT_REF",
    "supabase:pull": "supabase db pull",
    "supabase:reset": "supabase db reset"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@tailwindcss/typography": "^0.5.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.19.1",
    "lucide-react": "^0.522.0",
    "mongoose": "^8.16.0",
    "next": "15.3.4",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "type-fest": "^4.41.0",
    "zod": "^3.25.67",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.0",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "supabase": "^2.26.9",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}
